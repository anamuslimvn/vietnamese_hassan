[
  {
    "ID": "6107",
    "SURAH": "96",
    "AYAH": "1",
    "ARABIC_TEXT": "ٱقۡرَأۡ بِٱسۡمِ رَبِّكَ ٱلَّذِي خَلَقَ",
    "TRANSLATION": "Hãy đọc! Nhân danh Thượng Đế của Ngươi, Đấng đã tạo,",
    "FOOTNOTES": ""
  },
  {
    "ID": "6108",
    "SURAH": "96",
    "AYAH": "2",
    "ARABIC_TEXT": "خَلَقَ ٱلۡإِنسَٰنَ مِنۡ عَلَقٍ",
    "TRANSLATION": "Đã tạo con người từ một hòn máu đặc.",
    "FOOTNOTES": ""
  },
  {
    "ID": "6109",
    "SURAH": "96",
    "AYAH": "3",
    "ARABIC_TEXT": "ٱقۡرَأۡ وَرَبُّكَ ٱلۡأَكۡرَمُ",
    "TRANSLATION": "Hãy đọc! Và Thượng Đế của Ngươi Rất Mực Quảng Đại,",
    "FOOTNOTES": ""
  },
  {
    "ID": "6110",
    "SURAH": "96",
    "AYAH": "4",
    "ARABIC_TEXT": "ٱلَّذِي عَلَّمَ بِٱلۡقَلَمِ",
    "TRANSLATION": "Đấng đã dạy (kiến thức) bằng cây viết;",
    "FOOTNOTES": ""
  },
  {
    "ID": "6111",
    "SURAH": "96",
    "AYAH": "5",
    "ARABIC_TEXT": "عَلَّمَ ٱلۡإِنسَٰنَ مَا لَمۡ يَعۡلَمۡ",
    "TRANSLATION": "Đã dạy con người điều mà y không biết;",
    "FOOTNOTES": ""
  },
  {
    "ID": "6112",
    "SURAH": "96",
    "AYAH": "6",
    "ARABIC_TEXT": "كَلَّآ إِنَّ ٱلۡإِنسَٰنَ لَيَطۡغَىٰٓ",
    "TRANSLATION": "Nhưng không! Quả thật, con người thái quá,",
    "FOOTNOTES": ""
  },
  {
    "ID": "6113",
    "SURAH": "96",
    "AYAH": "7",
    "ARABIC_TEXT": "أَن رَّءَاهُ ٱسۡتَغۡنَىٰٓ",
    "TRANSLATION": "Bởi vì y tự thấy mình đầy đủ.",
    "FOOTNOTES": ""
  },
  {
    "ID": "6114",
    "SURAH": "96",
    "AYAH": "8",
    "ARABIC_TEXT": "إِنَّ إِلَىٰ رَبِّكَ ٱلرُّجۡعَىٰٓ",
    "TRANSLATION": "Quả thật, y sẽ trở lại gặp Thượng Đế của Ngươi (ở đời sau).",
    "FOOTNOTES": ""
  },
  {
    "ID": "6115",
    "SURAH": "96",
    "AYAH": "9",
    "ARABIC_TEXT": "أَرَءَيۡتَ ٱلَّذِي يَنۡهَىٰ",
    "TRANSLATION": "Há Ngươi có thấy một kẻ (Abu Jahl) đã ngăn cản",
    "FOOTNOTES": ""
  },
  {
    "ID": "6116",
    "SURAH": "96",
    "AYAH": "10",
    "ARABIC_TEXT": "عَبۡدًا إِذَا صَلَّىٰٓ",
    "TRANSLATION": "Một người bề tôi (Muhammad) khi Y dâng lễ nguyện Salah?",
    "FOOTNOTES": ""
  },
  {
    "ID": "6117",
    "SURAH": "96",
    "AYAH": "11",
    "ARABIC_TEXT": "أَرَءَيۡتَ إِن كَانَ عَلَى ٱلۡهُدَىٰٓ",
    "TRANSLATION": "Há Ngươi xét thấy y được hướng dẫn hay chăng?",
    "FOOTNOTES": ""
  },
  {
    "ID": "6118",
    "SURAH": "96",
    "AYAH": "12",
    "ARABIC_TEXT": "أَوۡ أَمَرَ بِٱلتَّقۡوَىٰٓ",
    "TRANSLATION": "Hoặc truyền bảo (người khác) nên sợ Allah hay chăng?",
    "FOOTNOTES": ""
  },
  {
    "ID": "6119",
    "SURAH": "96",
    "AYAH": "13",
    "ARABIC_TEXT": "أَرَءَيۡتَ إِن كَذَّبَ وَتَوَلَّىٰٓ",
    "TRANSLATION": "Há Ngươi xét thấy y phủ nhận sự thật và quay bỏ đi?",
    "FOOTNOTES": ""
  },
  {
    "ID": "6120",
    "SURAH": "96",
    "AYAH": "14",
    "ARABIC_TEXT": "أَلَمۡ يَعۡلَم بِأَنَّ ٱللَّهَ يَرَىٰ",
    "TRANSLATION": "Há y không biết rằng Allah Hằng thấy (tất cả).",
    "FOOTNOTES": ""
  },
  {
    "ID": "6121",
    "SURAH": "96",
    "AYAH": "15",
    "ARABIC_TEXT": "كَلَّا لَئِن لَّمۡ يَنتَهِ لَنَسۡفَعَۢا بِٱلنَّاصِيَةِ",
    "TRANSLATION": "Nhưng không! Nếu y không chịu ngưng (phá rối) thì chắc chắn TA sẽ nắm chùm tóc trán (của y),",
    "FOOTNOTES": ""
  },
  {
    "ID": "6122",
    "SURAH": "96",
    "AYAH": "16",
    "ARABIC_TEXT": "نَاصِيَةٖ كَٰذِبَةٍ خَاطِئَةٖ",
    "TRANSLATION": "Một chùm tóc láo khoét, tội lỗi.",
    "FOOTNOTES": ""
  },
  {
    "ID": "6123",
    "SURAH": "96",
    "AYAH": "17",
    "ARABIC_TEXT": "فَلۡيَدۡعُ نَادِيَهُۥ",
    "TRANSLATION": "Hãy để y gọi bè nhóm của y (đến giúp);",
    "FOOTNOTES": ""
  },
  {
    "ID": "6124",
    "SURAH": "96",
    "AYAH": "18",
    "ARABIC_TEXT": "سَنَدۡعُ ٱلزَّبَانِيَةَ",
    "TRANSLATION": "TA sẽ gọi các cảnh vệ của hỏa ngục (đến trừng trị)!",
    "FOOTNOTES": ""
  },
  {
    "ID": "6125",
    "SURAH": "96",
    "AYAH": "19",
    "ARABIC_TEXT": "كَلَّا لَا تُطِعۡهُ وَٱسۡجُدۡۤ وَٱقۡتَرِب۩",
    "TRANSLATION": "Nhưng không! Chớ nghe theo y. Và hãy phủ phục và đến gần (Ngài)",
    "FOOTNOTES": ""
  }
]