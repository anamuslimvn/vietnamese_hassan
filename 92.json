[
  {
    "ID": "6059",
    "SURAH": "92",
    "AYAH": "1",
    "ARABIC_TEXT": "وَٱلَّيۡلِ إِذَا يَغۡشَىٰ",
    "TRANSLATION": "Thề bởi ban đêm khi nó bao phủ; ",
    "FOOTNOTES": ""
  },
  {
    "ID": "6060",
    "SURAH": "92",
    "AYAH": "2",
    "ARABIC_TEXT": "وَٱلنَّهَارِ إِذَا تَجَلَّىٰ",
    "TRANSLATION": "Thề bởi ban ngày khi nó sáng tỏ;",
    "FOOTNOTES": ""
  },
  {
    "ID": "6061",
    "SURAH": "92",
    "AYAH": "3",
    "ARABIC_TEXT": "وَمَا خَلَقَ ٱلذَّكَرَ وَٱلۡأُنثَىٰٓ",
    "TRANSLATION": "Thề bởi Đấng đã tạo hóa nam và nữ giới;",
    "FOOTNOTES": ""
  },
  {
    "ID": "6062",
    "SURAH": "92",
    "AYAH": "4",
    "ARABIC_TEXT": "إِنَّ سَعۡيَكُمۡ لَشَتَّىٰ",
    "TRANSLATION": "Quả thật, nỗ lực của các ngươi rõ khác biệt;",
    "FOOTNOTES": ""
  },
  {
    "ID": "6063",
    "SURAH": "92",
    "AYAH": "5",
    "ARABIC_TEXT": "فَأَمَّا مَنۡ أَعۡطَىٰ وَٱتَّقَىٰ",
    "TRANSLATION": "Bởi thế, đối với ai bố thí và sợ Allah; ",
    "FOOTNOTES": ""
  },
  {
    "ID": "6064",
    "SURAH": "92",
    "AYAH": "6",
    "ARABIC_TEXT": "وَصَدَّقَ بِٱلۡحُسۡنَىٰ",
    "TRANSLATION": "Và tin nơi cái Tốt;",
    "FOOTNOTES": ""
  },
  {
    "ID": "6065",
    "SURAH": "92",
    "AYAH": "7",
    "ARABIC_TEXT": "فَسَنُيَسِّرُهُۥ لِلۡيُسۡرَىٰ",
    "TRANSLATION": "Thì TA sẽ làm con đường đi đến thoải mái được dễ dàng cho y;",
    "FOOTNOTES": ""
  },
  {
    "ID": "6066",
    "SURAH": "92",
    "AYAH": "8",
    "ARABIC_TEXT": "وَأَمَّا مَنۢ بَخِلَ وَٱسۡتَغۡنَىٰ",
    "TRANSLATION": "Ngược lại, đối với ai keo kiệt và tự cho mình giàu có đầy đủ.",
    "FOOTNOTES": ""
  },
  {
    "ID": "6067",
    "SURAH": "92",
    "AYAH": "9",
    "ARABIC_TEXT": "وَكَذَّبَ بِٱلۡحُسۡنَىٰ",
    "TRANSLATION": "Và phủ nhận cái Tốt",
    "FOOTNOTES": ""
  },
  {
    "ID": "6068",
    "SURAH": "92",
    "AYAH": "10",
    "ARABIC_TEXT": "فَسَنُيَسِّرُهُۥ لِلۡعُسۡرَىٰ",
    "TRANSLATION": "Thì TA sẽ làm con đường đưa đến cực nhọc được dễ dàng cho y;",
    "FOOTNOTES": ""
  },
  {
    "ID": "6069",
    "SURAH": "92",
    "AYAH": "11",
    "ARABIC_TEXT": "وَمَا يُغۡنِي عَنۡهُ مَالُهُۥٓ إِذَا تَرَدَّىٰٓ",
    "TRANSLATION": "Và của cải chẳng giúp ích gì được y khi y bị tiêu hủy;",
    "FOOTNOTES": ""
  },
  {
    "ID": "6070",
    "SURAH": "92",
    "AYAH": "12",
    "ARABIC_TEXT": "إِنَّ عَلَيۡنَا لَلۡهُدَىٰ",
    "TRANSLATION": "Quả thật, TA (Allah) có nhiệm vụ ban Chỉ Đạo hướng dẫn.",
    "FOOTNOTES": ""
  },
  {
    "ID": "6071",
    "SURAH": "92",
    "AYAH": "13",
    "ARABIC_TEXT": "وَإِنَّ لَنَا لَلۡأٓخِرَةَ وَٱلۡأُولَىٰ",
    "TRANSLATION": "Và quả thật, đời sau và đời này là của TA.",
    "FOOTNOTES": ""
  },
  {
    "ID": "6072",
    "SURAH": "92",
    "AYAH": "14",
    "ARABIC_TEXT": "فَأَنذَرۡتُكُمۡ نَارٗا تَلَظَّىٰ",
    "TRANSLATION": "Bởi thế, TA cảnh cáo các ngươi về ngọn Lửa cháy dữ dội.",
    "FOOTNOTES": ""
  },
  {
    "ID": "6073",
    "SURAH": "92",
    "AYAH": "15",
    "ARABIC_TEXT": "لَا يَصۡلَىٰهَآ إِلَّا ٱلۡأَشۡقَى",
    "TRANSLATION": "Không ai bị nướng (đốt) trong đó ngoài những kẻ bất hạnh nhất,",
    "FOOTNOTES": ""
  },
  {
    "ID": "6074",
    "SURAH": "92",
    "AYAH": "16",
    "ARABIC_TEXT": "ٱلَّذِي كَذَّبَ وَتَوَلَّىٰ",
    "TRANSLATION": "Tên phủ nhận sự thật và quay bỏ đi.",
    "FOOTNOTES": ""
  },
  {
    "ID": "6075",
    "SURAH": "92",
    "AYAH": "17",
    "ARABIC_TEXT": "وَسَيُجَنَّبُهَا ٱلۡأَتۡقَى",
    "TRANSLATION": "Và người ngay chính sợ Allah sẽ được giữ xa khỏi nó.",
    "FOOTNOTES": ""
  },
  {
    "ID": "6076",
    "SURAH": "92",
    "AYAH": "18",
    "ARABIC_TEXT": "ٱلَّذِي يُؤۡتِي مَالَهُۥ يَتَزَكَّىٰ",
    "TRANSLATION": "Người cho (bố thí) của cải của mình nhằm tẩy sạch (bản thân),",
    "FOOTNOTES": ""
  },
  {
    "ID": "6077",
    "SURAH": "92",
    "AYAH": "19",
    "ARABIC_TEXT": "وَمَا لِأَحَدٍ عِندَهُۥ مِن نِّعۡمَةٖ تُجۡزَىٰٓ",
    "TRANSLATION": "Và không đòi hỏi một ai trả ơn mình về ân huệ đã ban cho người,",
    "FOOTNOTES": ""
  },
  {
    "ID": "6078",
    "SURAH": "92",
    "AYAH": "20",
    "ARABIC_TEXT": "إِلَّا ٱبۡتِغَآءَ وَجۡهِ رَبِّهِ ٱلۡأَعۡلَىٰ",
    "TRANSLATION": "Mà chỉ mong tìm sắc diện của Thượng Đế của mình, Đấng Tối Cao;",
    "FOOTNOTES": ""
  },
  {
    "ID": "6079",
    "SURAH": "92",
    "AYAH": "21",
    "ARABIC_TEXT": "وَلَسَوۡفَ يَرۡضَىٰ",
    "TRANSLATION": "Và chắc chắn y sẽ sớm được hài lòng (vào thiên đàng).",
    "FOOTNOTES": ""
  }
]