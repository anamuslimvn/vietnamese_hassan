[
  {
    "ID": "5801",
    "SURAH": "81",
    "AYAH": "1",
    "ARABIC_TEXT": "إِذَا ٱلشَّمۡسُ كُوِّرَتۡ",
    "TRANSLATION": " Khi mặt trời cuốn xếp lại và biến mất; ",
    "FOOTNOTES": ""
  },
  {
    "ID": "5802",
    "SURAH": "81",
    "AYAH": "2",
    "ARABIC_TEXT": "وَإِذَا ٱلنُّجُومُ ٱنكَدَرَتۡ",
    "TRANSLATION": "Và khi các vì sao rơi rụng;",
    "FOOTNOTES": ""
  },
  {
    "ID": "5803",
    "SURAH": "81",
    "AYAH": "3",
    "ARABIC_TEXT": "وَإِذَا ٱلۡجِبَالُ سُيِّرَتۡ",
    "TRANSLATION": "Và khi các quả núi bị di chuyển mất biệt;",
    "FOOTNOTES": ""
  },
  {
    "ID": "5804",
    "SURAH": "81",
    "AYAH": "4",
    "ARABIC_TEXT": "وَإِذَا ٱلۡعِشَارُ عُطِّلَتۡ",
    "TRANSLATION": "Và khi các con lạc đà cái mang thai bị bỏ bê;",
    "FOOTNOTES": ""
  },
  {
    "ID": "5805",
    "SURAH": "81",
    "AYAH": "5",
    "ARABIC_TEXT": "وَإِذَا ٱلۡوُحُوشُ حُشِرَتۡ",
    "TRANSLATION": "Và khi các con dã thú sẽ được lùa tập trung lại;",
    "FOOTNOTES": ""
  },
  {
    "ID": "5806",
    "SURAH": "81",
    "AYAH": "6",
    "ARABIC_TEXT": "وَإِذَا ٱلۡبِحَارُ سُجِّرَتۡ",
    "TRANSLATION": "Và khi các đại dương dâng trào;",
    "FOOTNOTES": ""
  },
  {
    "ID": "5807",
    "SURAH": "81",
    "AYAH": "7",
    "ARABIC_TEXT": "وَإِذَا ٱلنُّفُوسُ زُوِّجَتۡ",
    "TRANSLATION": "Và khi các linh hồn sẽ được kết đôi với thân xác;",
    "FOOTNOTES": ""
  },
  {
    "ID": "5808",
    "SURAH": "81",
    "AYAH": "8",
    "ARABIC_TEXT": "وَإِذَا ٱلۡمَوۡءُۥدَةُ سُئِلَتۡ",
    "TRANSLATION": "Và khi các đứa bé gái bị chôn sống sẽ được tra hỏi-",
    "FOOTNOTES": ""
  },
  {
    "ID": "5809",
    "SURAH": "81",
    "AYAH": "9",
    "ARABIC_TEXT": "بِأَيِّ ذَنۢبٖ قُتِلَتۡ",
    "TRANSLATION": "Vì tội gì mà bé đã bị giết?",
    "FOOTNOTES": ""
  },
  {
    "ID": "5810",
    "SURAH": "81",
    "AYAH": "10",
    "ARABIC_TEXT": "وَإِذَا ٱلصُّحُفُ نُشِرَتۡ",
    "TRANSLATION": "Và khi các tờ giấy (ghi hành động) sẽ được mở ra;",
    "FOOTNOTES": ""
  },
  {
    "ID": "5811",
    "SURAH": "81",
    "AYAH": "11",
    "ARABIC_TEXT": "وَإِذَا ٱلسَّمَآءُ كُشِطَتۡ",
    "TRANSLATION": "Và khi bầu trời sẽ bị lột trần;",
    "FOOTNOTES": ""
  },
  {
    "ID": "5812",
    "SURAH": "81",
    "AYAH": "12",
    "ARABIC_TEXT": "وَإِذَا ٱلۡجَحِيمُ سُعِّرَتۡ",
    "TRANSLATION": "Và khi hỏa ngục sẽ được nhúm thành lửa ngọn;",
    "FOOTNOTES": ""
  },
  {
    "ID": "5813",
    "SURAH": "81",
    "AYAH": "13",
    "ARABIC_TEXT": "وَإِذَا ٱلۡجَنَّةُ أُزۡلِفَتۡ",
    "TRANSLATION": "Và khi thiên đàng sẽ được mang đến gần,",
    "FOOTNOTES": ""
  },
  {
    "ID": "5814",
    "SURAH": "81",
    "AYAH": "14",
    "ARABIC_TEXT": "عَلِمَتۡ نَفۡسٞ مَّآ أَحۡضَرَتۡ",
    "TRANSLATION": "Mỗi người sẽ biết điều mà y trình bày.",
    "FOOTNOTES": ""
  },
  {
    "ID": "5815",
    "SURAH": "81",
    "AYAH": "15",
    "ARABIC_TEXT": "فَلَآ أُقۡسِمُ بِٱلۡخُنَّسِ",
    "TRANSLATION": "Bởi thế, TA thề bởi các hành tinh lặn khuất,",
    "FOOTNOTES": ""
  },
  {
    "ID": "5816",
    "SURAH": "81",
    "AYAH": "16",
    "ARABIC_TEXT": "ٱلۡجَوَارِ ٱلۡكُنَّسِ",
    "TRANSLATION": "Di chuyển nhanh biến mất,",
    "FOOTNOTES": ""
  },
  {
    "ID": "5817",
    "SURAH": "81",
    "AYAH": "17",
    "ARABIC_TEXT": "وَٱلَّيۡلِ إِذَا عَسۡعَسَ",
    "TRANSLATION": "Và bởi ban đêm khi nó ra đi;",
    "FOOTNOTES": ""
  },
  {
    "ID": "5818",
    "SURAH": "81",
    "AYAH": "18",
    "ARABIC_TEXT": "وَٱلصُّبۡحِ إِذَا تَنَفَّسَ",
    "TRANSLATION": "Và bởi ban mai khi nó thổi (màn đêm đi mất) và sáng tỏ;",
    "FOOTNOTES": ""
  },
  {
    "ID": "5819",
    "SURAH": "81",
    "AYAH": "19",
    "ARABIC_TEXT": "إِنَّهُۥ لَقَوۡلُ رَسُولٖ كَرِيمٖ",
    "TRANSLATION": "Quả thật, Nó (Qur'an) là Lời phán của (Allah do) một Vị Thiên sứ (Jibril) rất vinh dự (đã mang đến),",
    "FOOTNOTES": ""
  },
  {
    "ID": "5820",
    "SURAH": "81",
    "AYAH": "20",
    "ARABIC_TEXT": "ذِي قُوَّةٍ عِندَ ذِي ٱلۡعَرۡشِ مَكِينٖ",
    "TRANSLATION": "Được Chủ Nhân của Ngai Vương ban cho quyền lực và danh dự,",
    "FOOTNOTES": ""
  },
  {
    "ID": "5821",
    "SURAH": "81",
    "AYAH": "21",
    "ARABIC_TEXT": "مُّطَاعٖ ثَمَّ أَمِينٖ",
    "TRANSLATION": "Được (các Thiên Thần) tuân theo nơi đó và rất đáng tin cậy.",
    "FOOTNOTES": ""
  },
  {
    "ID": "5822",
    "SURAH": "81",
    "AYAH": "22",
    "ARABIC_TEXT": "وَمَا صَاحِبُكُم بِمَجۡنُونٖ",
    "TRANSLATION": "Và người Bạn (Muhammad) của các ngươi không phải là một kẻ điên.",
    "FOOTNOTES": ""
  },
  {
    "ID": "5823",
    "SURAH": "81",
    "AYAH": "23",
    "ARABIC_TEXT": "وَلَقَدۡ رَءَاهُ بِٱلۡأُفُقِ ٱلۡمُبِينِ",
    "TRANSLATION": "Và quả thật, Y đã thấy vị (Jibril) tại chân trời sáng tỏ.",
    "FOOTNOTES": ""
  },
  {
    "ID": "5824",
    "SURAH": "81",
    "AYAH": "24",
    "ARABIC_TEXT": "وَمَا هُوَ عَلَى ٱلۡغَيۡبِ بِضَنِينٖ",
    "TRANSLATION": "Và Y không giữ cho riêng mình điều huyền bí (vô hình).",
    "FOOTNOTES": ""
  },
  {
    "ID": "5825",
    "SURAH": "81",
    "AYAH": "25",
    "ARABIC_TEXT": "وَمَا هُوَ بِقَوۡلِ شَيۡطَٰنٖ رَّجِيمٖ",
    "TRANSLATION": "Và Nó (Qur'an) không là lời nói của Shaytan đáng tống xuất.",
    "FOOTNOTES": ""
  },
  {
    "ID": "5826",
    "SURAH": "81",
    "AYAH": "26",
    "ARABIC_TEXT": "فَأَيۡنَ تَذۡهَبُونَ",
    "TRANSLATION": "Thế các ngươi đi đâu?",
    "FOOTNOTES": ""
  },
  {
    "ID": "5827",
    "SURAH": "81",
    "AYAH": "27",
    "ARABIC_TEXT": "إِنۡ هُوَ إِلَّا ذِكۡرٞ لِّلۡعَٰلَمِينَ",
    "TRANSLATION": "Quả thật, Nó (Qur'an) chỉ là Lời Cảnh Tỉnh nhắc nhỡ muôn loài.",
    "FOOTNOTES": ""
  },
  {
    "ID": "5828",
    "SURAH": "81",
    "AYAH": "28",
    "ARABIC_TEXT": "لِمَن شَآءَ مِنكُمۡ أَن يَسۡتَقِيمَ",
    "TRANSLATION": "Nhắc nhở ai trong các ngươi muốn đi đúng đường,",
    "FOOTNOTES": ""
  },
  {
    "ID": "5829",
    "SURAH": "81",
    "AYAH": "29",
    "ARABIC_TEXT": "وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ رَبُّ ٱلۡعَٰلَمِينَ",
    "TRANSLATION": "Nhưng điều mà các ngươi muốn sẽ không thành tựu trừ phi Allah, Thượng Đế của vũ trụ và muôn loài muốn (hay chấp thuận).",
    "FOOTNOTES": ""
  }
]