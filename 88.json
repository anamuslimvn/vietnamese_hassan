[
  {
    "ID": "5968",
    "SURAH": "88",
    "AYAH": "1",
    "ARABIC_TEXT": "هَلۡ أَتَىٰكَ حَدِيثُ ٱلۡغَٰشِيَةِ",
    "TRANSLATION": "Ngươi có (nghe) câu chuyện về hiện tượng bao trùm hay chưa?",
    "FOOTNOTES": ""
  },
  {
    "ID": "5969",
    "SURAH": "88",
    "AYAH": "2",
    "ARABIC_TEXT": "وُجُوهٞ يَوۡمَئِذٍ خَٰشِعَةٌ",
    "TRANSLATION": "Vào Ngày đó, gương mặt (của một số người) sẽ sa sầm.",
    "FOOTNOTES": ""
  },
  {
    "ID": "5970",
    "SURAH": "88",
    "AYAH": "3",
    "ARABIC_TEXT": "عَامِلَةٞ نَّاصِبَةٞ",
    "TRANSLATION": "Cực nhọc, rũ rượi. ",
    "FOOTNOTES": ""
  },
  {
    "ID": "5971",
    "SURAH": "88",
    "AYAH": "4",
    "ARABIC_TEXT": "تَصۡلَىٰ نَارًا حَامِيَةٗ",
    "TRANSLATION": "Sẽ đi vào trong Lửa cháy bùng,",
    "FOOTNOTES": ""
  },
  {
    "ID": "5972",
    "SURAH": "88",
    "AYAH": "5",
    "ARABIC_TEXT": "تُسۡقَىٰ مِنۡ عَيۡنٍ ءَانِيَةٖ",
    "TRANSLATION": "Sẽ được cho uống nước (sôi) múc từ một ngọn suối nóng bỏng,",
    "FOOTNOTES": ""
  },
  {
    "ID": "5973",
    "SURAH": "88",
    "AYAH": "6",
    "ARABIC_TEXT": "لَّيۡسَ لَهُمۡ طَعَامٌ إِلَّا مِن ضَرِيعٖ",
    "TRANSLATION": "Không có thức ăn nào cho họ dùng ngoài loại cây đắng gai góc",
    "FOOTNOTES": ""
  },
  {
    "ID": "5974",
    "SURAH": "88",
    "AYAH": "7",
    "ARABIC_TEXT": "لَّا يُسۡمِنُ وَلَا يُغۡنِي مِن جُوعٖ",
    "TRANSLATION": "Không dinh dưỡng cũng không làm cho hết đói.",
    "FOOTNOTES": ""
  },
  {
    "ID": "5975",
    "SURAH": "88",
    "AYAH": "8",
    "ARABIC_TEXT": "وُجُوهٞ يَوۡمَئِذٖ نَّاعِمَةٞ",
    "TRANSLATION": "Vào Ngày đó, gương mặt (của một số người) sẽ hân hoan,",
    "FOOTNOTES": ""
  },
  {
    "ID": "5976",
    "SURAH": "88",
    "AYAH": "9",
    "ARABIC_TEXT": "لِّسَعۡيِهَا رَاضِيَةٞ",
    "TRANSLATION": "Mãn nguyện với nỗ lực của mình. ",
    "FOOTNOTES": ""
  },
  {
    "ID": "5977",
    "SURAH": "88",
    "AYAH": "10",
    "ARABIC_TEXT": "فِي جَنَّةٍ عَالِيَةٖ",
    "TRANSLATION": "Trong một ngôi vườn cao đẹp.",
    "FOOTNOTES": ""
  },
  {
    "ID": "5978",
    "SURAH": "88",
    "AYAH": "11",
    "ARABIC_TEXT": "لَّا تَسۡمَعُ فِيهَا لَٰغِيَةٗ",
    "TRANSLATION": "Mà họ sẽ không nghe một lời nhảm nhí nào,",
    "FOOTNOTES": ""
  },
  {
    "ID": "5979",
    "SURAH": "88",
    "AYAH": "12",
    "ARABIC_TEXT": "فِيهَا عَيۡنٞ جَارِيَةٞ",
    "TRANSLATION": "Bên trong có suối nước chảy, ",
    "FOOTNOTES": ""
  },
  {
    "ID": "5980",
    "SURAH": "88",
    "AYAH": "13",
    "ARABIC_TEXT": "فِيهَا سُرُرٞ مَّرۡفُوعَةٞ",
    "TRANSLATION": "Có bệ ngồi cao,",
    "FOOTNOTES": ""
  },
  {
    "ID": "5981",
    "SURAH": "88",
    "AYAH": "14",
    "ARABIC_TEXT": "وَأَكۡوَابٞ مَّوۡضُوعَةٞ",
    "TRANSLATION": "Có cốc nước giải khát dọn sẵn. ",
    "FOOTNOTES": ""
  },
  {
    "ID": "5982",
    "SURAH": "88",
    "AYAH": "15",
    "ARABIC_TEXT": "وَنَمَارِقُ مَصۡفُوفَةٞ",
    "TRANSLATION": "Có gối tựa sắp thành hàng,",
    "FOOTNOTES": ""
  },
  {
    "ID": "5983",
    "SURAH": "88",
    "AYAH": "16",
    "ARABIC_TEXT": "وَزَرَابِيُّ مَبۡثُوثَةٌ",
    "TRANSLATION": "Có thảm trải xinh đẹp.",
    "FOOTNOTES": ""
  },
  {
    "ID": "5984",
    "SURAH": "88",
    "AYAH": "17",
    "ARABIC_TEXT": "أَفَلَا يَنظُرُونَ إِلَى ٱلۡإِبِلِ كَيۡفَ خُلِقَتۡ",
    "TRANSLATION": "Thế họ không quan sát các con lạc đà, xem chúng đã được tạo hóa như thế nào ư?",
    "FOOTNOTES": ""
  },
  {
    "ID": "5985",
    "SURAH": "88",
    "AYAH": "18",
    "ARABIC_TEXT": "وَإِلَى ٱلسَّمَآءِ كَيۡفَ رُفِعَتۡ",
    "TRANSLATION": "Và bầu trời đã được nhấc cao như thế nào ư?",
    "FOOTNOTES": ""
  },
  {
    "ID": "5986",
    "SURAH": "88",
    "AYAH": "19",
    "ARABIC_TEXT": "وَإِلَى ٱلۡجِبَالِ كَيۡفَ نُصِبَتۡ",
    "TRANSLATION": "Và các quả núi đã được dựng đứng như thế nào ư?",
    "FOOTNOTES": ""
  },
  {
    "ID": "5987",
    "SURAH": "88",
    "AYAH": "20",
    "ARABIC_TEXT": "وَإِلَى ٱلۡأَرۡضِ كَيۡفَ سُطِحَتۡ",
    "TRANSLATION": "Và trái đất đã được trải rộng như thế nào ư?",
    "FOOTNOTES": ""
  },
  {
    "ID": "5988",
    "SURAH": "88",
    "AYAH": "21",
    "ARABIC_TEXT": "فَذَكِّرۡ إِنَّمَآ أَنتَ مُذَكِّرٞ",
    "TRANSLATION": "Hãy nhắc nhở (họ); Ngươi chỉ là một Người Nhắc Nhở;",
    "FOOTNOTES": ""
  },
  {
    "ID": "5989",
    "SURAH": "88",
    "AYAH": "22",
    "ARABIC_TEXT": "لَّسۡتَ عَلَيۡهِم بِمُصَيۡطِرٍ",
    "TRANSLATION": "Chứ không phải là một vị giám đốc (độc tài) đối với họ.",
    "FOOTNOTES": ""
  },
  {
    "ID": "5990",
    "SURAH": "88",
    "AYAH": "23",
    "ARABIC_TEXT": "إِلَّا مَن تَوَلَّىٰ وَكَفَرَ",
    "TRANSLATION": "Trừ phi ai quay bỏ đi và không tin tưởng",
    "FOOTNOTES": ""
  },
  {
    "ID": "5991",
    "SURAH": "88",
    "AYAH": "24",
    "ARABIC_TEXT": "فَيُعَذِّبُهُ ٱللَّهُ ٱلۡعَذَابَ ٱلۡأَكۡبَرَ",
    "TRANSLATION": "Thì Allah sẽ trừng phạt y bằng một hình phạt to lớn.",
    "FOOTNOTES": ""
  },
  {
    "ID": "5992",
    "SURAH": "88",
    "AYAH": "25",
    "ARABIC_TEXT": "إِنَّ إِلَيۡنَآ إِيَابَهُمۡ",
    "TRANSLATION": "Quả thật, họ sẽ trở về gặp TA",
    "FOOTNOTES": ""
  },
  {
    "ID": "5993",
    "SURAH": "88",
    "AYAH": "26",
    "ARABIC_TEXT": "ثُمَّ إِنَّ عَلَيۡنَا حِسَابَهُم",
    "TRANSLATION": "Rồi, TA có nhiệm vụ thanh toán họ.",
    "FOOTNOTES": ""
  }
]