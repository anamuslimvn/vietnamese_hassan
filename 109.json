[
  {
    "ID": "6208",
    "SURAH": "109",
    "AYAH": "1",
    "ARABIC_TEXT": "قُلۡ يَٰٓأَيُّهَا ٱلۡكَٰفِرُونَ",
    "TRANSLATION": "Hãy bảo (chúng): “Này hỡi những kẻ phủ nhận Allah!\"",
    "FOOTNOTES": ""
  },
  {
    "ID": "6209",
    "SURAH": "109",
    "AYAH": "2",
    "ARABIC_TEXT": "لَآ أَعۡبُدُ مَا تَعۡبُدُونَ",
    "TRANSLATION": "\"Ta không tôn thờ những kẻ (thần linh) mà các ngươi tôn thờ.\"",
    "FOOTNOTES": ""
  },
  {
    "ID": "6210",
    "SURAH": "109",
    "AYAH": "3",
    "ARABIC_TEXT": "وَلَآ أَنتُمۡ عَٰبِدُونَ مَآ أَعۡبُدُ",
    "TRANSLATION": "“Các ngươi cũng sẽ không tôn thờ Đấng (Allah) mà Ta thờ phụng.\"",
    "FOOTNOTES": ""
  },
  {
    "ID": "6211",
    "SURAH": "109",
    "AYAH": "4",
    "ARABIC_TEXT": "وَلَآ أَنَا۠ عَابِدٞ مَّا عَبَدتُّمۡ",
    "TRANSLATION": "“Và Ta sẽ không là một tín đồ của kẻ mà các ngươi đang tôn thờ.\"",
    "FOOTNOTES": ""
  },
  {
    "ID": "6212",
    "SURAH": "109",
    "AYAH": "5",
    "ARABIC_TEXT": "وَلَآ أَنتُمۡ عَٰبِدُونَ مَآ أَعۡبُدُ",
    "TRANSLATION": "“Và các ngươi cũng sẽ không tôn thờ Đấng mà Ta đang thờ phụng.'",
    "FOOTNOTES": ""
  },
  {
    "ID": "6213",
    "SURAH": "109",
    "AYAH": "6",
    "ARABIC_TEXT": "لَكُمۡ دِينُكُمۡ وَلِيَ دِينِ",
    "TRANSLATION": "“Các ngươi có tôn giáo của các ngươi và Ta có tôn giáo của Ta.”",
    "FOOTNOTES": ""
  }
]