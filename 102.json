[
  {
    "ID": "6169",
    "SURAH": "102",
    "AYAH": "1",
    "ARABIC_TEXT": "أَلۡهَىٰكُمُ ٱلتَّكَاثُرُ",
    "TRANSLATION": "Việc tom góp của cải (làm giàu) làm cho các ngươi bận tâm,",
    "FOOTNOTES": ""
  },
  {
    "ID": "6170",
    "SURAH": "102",
    "AYAH": "2",
    "ARABIC_TEXT": "حَتَّىٰ زُرۡتُمُ ٱلۡمَقَابِرَ",
    "TRANSLATION": "Cho đến khi các ngươi đi thăm mộ.",
    "FOOTNOTES": ""
  },
  {
    "ID": "6171",
    "SURAH": "102",
    "AYAH": "3",
    "ARABIC_TEXT": "كَلَّا سَوۡفَ تَعۡلَمُونَ",
    "TRANSLATION": "Nhất định không! Các ngươi sẽ sớm biết!",
    "FOOTNOTES": ""
  },
  {
    "ID": "6172",
    "SURAH": "102",
    "AYAH": "4",
    "ARABIC_TEXT": "ثُمَّ كَلَّا سَوۡفَ تَعۡلَمُونَ",
    "TRANSLATION": "Rồi nhất định không! Các ngươi sẽ sớm biết!",
    "FOOTNOTES": ""
  },
  {
    "ID": "6173",
    "SURAH": "102",
    "AYAH": "5",
    "ARABIC_TEXT": "كَلَّا لَوۡ تَعۡلَمُونَ عِلۡمَ ٱلۡيَقِينِ",
    "TRANSLATION": "Nhất định không! Nếu các ngươi biết với một sự hiểu biết chắc chắn",
    "FOOTNOTES": ""
  },
  {
    "ID": "6174",
    "SURAH": "102",
    "AYAH": "6",
    "ARABIC_TEXT": "لَتَرَوُنَّ ٱلۡجَحِيمَ",
    "TRANSLATION": "Thì chắc chắn sẽ thấy hỏa ngục!",
    "FOOTNOTES": ""
  },
  {
    "ID": "6175",
    "SURAH": "102",
    "AYAH": "7",
    "ARABIC_TEXT": "ثُمَّ لَتَرَوُنَّهَا عَيۡنَ ٱلۡيَقِينِ",
    "TRANSLATION": "Rồi chắc chắn các ngươi sẽ thấy nó với cặp mắt kiên định!",
    "FOOTNOTES": ""
  },
  {
    "ID": "6176",
    "SURAH": "102",
    "AYAH": "8",
    "ARABIC_TEXT": "ثُمَّ لَتُسۡـَٔلُنَّ يَوۡمَئِذٍ عَنِ ٱلنَّعِيمِ",
    "TRANSLATION": "Rồi chắc chắn vào Ngày đó các ngươi sẽ bị tra hỏi về lạc thú.",
    "FOOTNOTES": ""
  }
]